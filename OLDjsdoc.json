{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
      "include": ["./client/src/"],
      "includePattern": ".js",
      "excludePattern": "node_modules/ |docs"
  },
  "plugins": ["plugins/markdown", "better-docs/component"], 
  "opts": {
    "recurse": true,
    "destination": "./docs/"
    
  }
}