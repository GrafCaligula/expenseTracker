{
  "plugins": ["plugins/markdown", "node_modules/better-docs/component"],
  "source": {
      "include": ["./client/src/"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "verbose": true,
    "destination": "./docs/",
    "template": "docdash"   
    
  }
} 